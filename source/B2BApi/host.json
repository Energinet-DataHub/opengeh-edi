{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "%OrchestrationsTaskHubName%",
      "storageProvider": {
        "connectionStringName": "OrchestrationsStorageConnectionString"
      },
      "tracing": {
        "traceInputsAndOutputs": true
      },
      "storeInputsInOrchestrationHistory": true
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Exception"
      }
    }
  }
}

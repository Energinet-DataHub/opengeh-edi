<xsd:schema
   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:rsm="un:unece:260:data:EEM"
   xmlns:ccts="urn:un:unece:uncefact:documentation:common:3:standard:CoreComponentsTechnicalSpecification:3"
   xmlns:xbt="urn:un:unece:uncefact:data:common:1:draft"
   targetNamespace="un:unece:260:data:EEM"
   elementFormDefault="qualified"
   attributeFormDefault="unqualified"
   version="0.9.C">
   <!-- =============================================================================== -->
   <!-- ==== Imports                                                               ==== -->
   <!-- =============================================================================== -->

   <!-- =============================================================================== -->
   <!-- ==== Inclusions                                                            ==== -->
   <!-- =============================================================================== -->
   <!-- =============================================================================== -->
   <!-- ==== Inclusion of Aggregated Business Information Entities                 ==== -->
   <!-- =============================================================================== -->
   <xsd:include schemaLocation="ebIX_AggregatedBusinessInformationEntities_0p1pA.xsd"/>
   <!-- =============================================================================== -->
   <!-- ==== Inclusion of Business Data Types                                      ==== -->
   <!-- =============================================================================== -->
   <xsd:include schemaLocation="ebIX_BusinessDataType_0p1pA.xsd"/>
   <!-- =============================================================================== -->
   <!-- ==== Element Declarations                                                  ==== -->
   <!-- =============================================================================== -->
   <!-- =============================================================================== -->
   <!-- ==== Root Element Declarations                                             ==== -->
   <!-- =============================================================================== -->
   <!-- =============================================================================== -->
   <!-- ==== DK_RejectUpdateMasterDataMeteringPoint                                ==== -->
   <!-- =============================================================================== -->
   <!-- =============================================================================== -->
   <!-- ==== Root Element                                                          ==== -->
   <!-- =============================================================================== -->
   <xsd:element name="DK_RejectUpdateMasterDataMeteringPoint" type="rsm:DK_RejectUpdateMasterDataMeteringPointType"/>
   <!-- =============================================================================== -->
   <!-- ==== Complex Element                                                       ==== -->
   <!-- =============================================================================== -->
   <xsd:complexType name="DK_RejectUpdateMasterDataMeteringPointType">
      <xsd:sequence>
         <xsd:element ref="rsm:HeaderEnergyDocument" minOccurs="1" maxOccurs="1"/>
         <xsd:element ref="rsm:ProcessEnergyContext" minOccurs="1" maxOccurs="1"/>
         <xsd:element ref="rsm:PayloadResponseEvent" minOccurs="1" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <!-- =============================================================================== -->
   <xsd:element name="HeaderEnergyDocument" type="rsm:EnergyDocument"/>
   <xsd:element name="ProcessEnergyContext" type="rsm:EnergyContext"/>
   <xsd:element name="PayloadResponseEvent" type="rsm:ResponseEvent"/>
</xsd:schema>  

{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request;Exception;Trace"
            }
        }
    },
    "retry": {
      "maxRetryCount": 10,
      "strategy": "fixedDelay",
      "delayInterval": "00:00:15"
  }
}
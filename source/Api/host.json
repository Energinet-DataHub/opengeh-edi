{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request;Exception"
            }
        }
    },
  "extensions": {
    "serviceBus": {
      "messageHandlerOptions": {
        "maxConcurrentCalls": 8
      },
      "sessionHandlerOptions": {
        "maxConcurrentSessions": 8
      }
    }
  }
}
